{
  "name": "klg-tracer",
  "version": "3.1.0",
  "description": "tracer log ",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "lint": "tslint -c tslint.json 'src/**/*.ts' --fix",
    "build": "rm -rf ./dist && tsc -d -p ./",
    "test": "DEBUG=PandoraHook:* TS_NODE_CACHE=true mocha -t 100000",
    "prepublishOnly": "npm run build"
  },
  "jest": {
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "json"
    ],
    "transform": {
      "^.+\\.tsx?$": "<rootDir>/node_modules/ts-jest/preprocessor.js"
    },
    "testRegex": "/src/.*\\.(test|spec).(ts|tsx|js)$",
    "collectCoverageFrom": [
      "src/**/*.{js,jsx,tsx,ts}",
      "!**/node_modules/**",
      "!**/vendor/**"
    ],
    "coverageReporters": [
      "json",
      "lcov"
    ]
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/kaolalicai/klg-tracer"
  },
  "keywords": [
    "klg"
  ],
  "author": "nick",
  "license": "MIT",
  "engines": {
    "node": ">=8.9.0"
  },
  "dependencies": {
    "@koa/cors": "2",
    "klg-logger": "^1.0.4",
    "klg-tracer-model": "^2.0.0",
    "pandora-hook": "^1.10.6",
    "pandora-hub": "^1.10.6",
    "pandora-metrics": "^1.10.6"
  },
  "devDependencies": {
    "@types/mocha": "^5.2.0",
    "@types/mongoose": "^5.0.9",
    "@types/node": "^9.6.4",
    "@types/sinon": "^4.3.1",
    "espower-typescript": "^8.1.3",
    "mocha": "^5.1.1",
    "mongodb-mock-server": "^1.0.0",
    "mongoose": "^4.13.10",
    "nock": "^9.2.5",
    "nyc": "^11.7.1",
    "nyc-ts-patch": "^1.0.0",
    "power-assert": "^1.5.0",
    "sinon": "^5.0.3",
    "ts-node": "^5.0.1",
    "tslint": "^5.9.1",
    "tslint-config-klg": "^1.0.4",
    "typescript": "^2.8.1",
    "urllib": "^2.27.0",
    "webstorm-disable-index": "^1.2.0"
  }
}
